{
  "name": "honyaku",
  "version": "1.0.0",
  "displayName": "Honyaku",
  "author": "CorvusGlaive",
  "description": "A failed attempt of making an extension",
  "scripts": {
    "build:chrome": "cross-env VITE_BROWSER=chrome vite build",
    "build:firefox": "cross-env VITE_BROWSER=firefox vite build",
    "build": "npm run build:chrome & npm run build:firefox",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run -f firefox-developer-edition --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "pack": "web-ext build -s ./dist/ -a ./dist/pack --overwrite-dest"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "5.x",
    "@sveltejs/vite-plugin-svelte": "2.x",
    "@tsconfig/svelte": "5.x",
    "@types/lodash-es": "^4.17.8",
    "@types/wanakana": "^4.0.3",
    "@types/webextension-polyfill": "^0.10.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "prettier-plugin-tailwindcss": "0.4.x",
    "svelte": "^4.1.1",
    "svelte-check": "3.x",
    "svelte-preprocess": "5.x",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.0",
    "typescript": "5.1.x",
    "typescript-svelte-plugin": "^0.3.29",
    "vite": "^4.4.6",
    "web-ext": "7.x"
  },
  "dependencies": {
    "@egjs/svelte-grid": "^1.14.1",
    "@floating-ui/dom": "^1.4.5",
    "@fontsource-variable/roboto-flex": "^5.0.5",
    "clsx": "^2.0.0",
    "colord": "^2.9.3",
    "fuse.js": "^6.6.2",
    "idb": "7.x",
    "ky": ">=0.33.3",
    "lodash-es": "^4.17.21",
    "svelte-spa-router": "^3.3.0",
    "wanakana": "^5.1.0",
    "webextension-polyfill": "^0.10.0"
  },
  "type": "module"
}
